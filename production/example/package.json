  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env.test\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@types/node": "^17.0.8",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@types/uuid": "^8.3.3",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "mongoose": "^6.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "uuid": "^8.3.2",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "dotenv": "^10.0.0",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "prettier": "^2.5.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0"
  },
  "dockerBuildOptions": {
    "buildArgs": [
      "TEST_ARG=test"
    ]
  }
