{
  "name": "example",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env.test\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "8.1.2",
    "@nestjs/core": "8.1.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/microservices": "8.1.2",
    "@nestjs/mongoose": "9.0.1",
    "@nestjs/platform-express": "8.1.2",
    "@types/uuid": "^8.3.1",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "mongoose": "6.0.12",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "16.11.5",
    "dotenv": "10.0.0",
    "ts-node": "10.4.0",
    "tsc-watch": "4.5.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4",
    "prettier": "2.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0"
  },
  "dockerBuildOptions": {
    "buildArgs": [
      "--build-arg",
      "TEST_ARG=test"
    ]
  }
}
